<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#11ddf5" />
            <!--<stroke android:width="1dip" android:color="#FCA12C" />-->
            <gradient android:angle="-90" android:startColor="#f00886" android:endColor="#f00886"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#11ddf5" />
            <solid android:color="#11ddf5"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <!--<stroke android:width="1dip" android:color="#FCA12C" />-->
            <stroke android:width="1dip" android:color="#11ddf5" />
            <gradient android:angle="-90" android:startColor="#F00886" android:endColor="#F00886" />
        </shape>
    </item>
</selector>